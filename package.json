{
  "name": "react-chakra-pagination",
  "version": "1.1.3",
  "description": "Easy way for paginate lists using Chakra UI, using lists components and hooks.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnzsss/react-chakra-pagination.git"
  },
  "homepage": "https://github.com/mnzsss/react-chakra-pagination#readme",
  "license": "ISC",
  "keywords": [
    "chakra-ui",
    "react",
    "pagination",
    "react-table"
  ],
  "files": [
    "/dist"
  ],
  "author": "Gabriel Menezes",
  "dependencies": {
    "@chakra-ui/react": "^1.6.9",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-table": "^7.7.6",
    "tsup": "^6.5.0",
    "typescript": "^4.4.3"
  }
}
