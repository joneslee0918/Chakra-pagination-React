{
  "name": "react-chakra-pagination",
  "version": "2.0.1",
  "author": "Gabriel Menezes <gabrielsmenezes99@gmail.com>",
  "license": "MIT",
  "description": "Easy way for paginate lists using Chakra UI, using lists components and hooks.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublish": "rm -rf ./dist && pnpm run build",
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "changeset": "changeset",
    "release": "yarn build && yarn release-it",
    "lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnzsss/react-chakra-pagination.git"
  },
  "homepage": "https://github.com/mnzsss/react-chakra-pagination#readme",
  "keywords": [
    "chakra-ui",
    "react",
    "pagination",
    "react-table"
  ],
  "files": [
    "/dist"
  ],
  "peerDependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "framer-motion": "^8.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.16",
    "@tanstack/react-table": "^8.7.6",
    "csstype": "^3.1.1"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@changesets/cli": "^2.26.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "framer-motion": "^8.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.4.3",
    "tsup": "^6.5.0",
    "release-it": "15.6.0"
  }
}
