{
  "name": "react-chakra-pagination",
  "version": "1.1.2",
  "description": "Easy way for paginate lists using Chakra UI, using lists components and hooks.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "start": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnzsss/react-chakra-pagination.git"
  },
  "homepage": "https://github.com/mnzsss/react-chakra-pagination#readme",
  "license": "ISC",
  "keywords": [
    "chakra-ui",
    "react",
    "pagination",
    "react-table"
  ],
  "files": [
    "/lib"
  ],
  "author": "Gabriel Menezes",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@chakra-ui/react": "^1.6.9",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@types/react-table": "^7.7.6",
    "rollup": "^2.58.0",
    "rollup-plugin-sass": "^1.2.9",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9"
  }
}
